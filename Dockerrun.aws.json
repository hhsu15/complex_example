{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
	 "name": "client",
	 "image": "hhsu15/multi-client",
	 "hostname": "client",
	 "essential": false
	},
    {
	 "name": "server",
	 "image": "hhsu15/multi-server",
	 "hostname": "api",
	 "essential": false
	},
    {
	 "name": "worker",
	 "image": "hhsu15/multi-worker",
	 "hostname": "worker",
	 "essential": false
	},
	{
	"name": "nginx",
	"image": "hhsu15/multi-nginx",
	"hostname": "nginx",
	"essential": true,
    "portMappings": [
	  {
	    "hostPort": 80,
		"containerPort": 80
	  }
	],
	"links":["client", "server"]
	}
  ]
}

